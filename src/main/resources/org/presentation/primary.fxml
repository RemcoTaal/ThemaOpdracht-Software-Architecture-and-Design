<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.presentation.PrimaryController">
   <children>
      <Label text="Welkom" />
      <Button fx:id="defineButton" text="Maak een nieuwe business rule" onAction="#switchToDefine"/>
      <Button fx:id="generateButton" text="Genereer een businessRule" onAction="#switchToGenerate"/>
   </children>

   <padding>
      <Insets bottom="100.0" left="100.0" right="100.0" top="100.0" />
   </padding>
</VBox>
